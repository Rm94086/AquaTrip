    <!-- Main Content -->
    <main class="main-content">
        <!-- Hero Section -->
        <section class="hero-section">
            <section class="hero-image">
                <img src="img/aquariosbingo.jpg" alt="Aquário subaquático" />
                <section class="hero-overlay">
                    <h1 class="hero-title">Explore os melhores aquários do mundo</h1>
                </section>
            </section>
        </section>
        <!-- Featured Section -->
        <section class="featured-section">
            <h2 class="section-title">DESTAQUE DO AQUATRIP</h2>
            <h2 class="section-title2">Sea Life Bangkok Ocean World</h2>
            
            <section class="featured-grid">
                <section class="featured-card large">
                    <img src="img/bangkok3.webp" alt="Sea Life Bangkok - Tubarões" />
                </section>
                
                <section class="featured-card small">
                    <img src="img/bangkok1.jpg" alt="Sea Life Bangkok - Vida marinha" />
                </section>
                
                <section class="featured-card small">
                    <img src="img/bangkok2.jpg" alt="Sea Life Bangkok - Aquário" />
                </section>
            </section>
        </section>

        <!-- Discovery Section -->
        <section class="discovery-section">
            <h2 class="section-title">Descubra seu próximo passeio</h2>

    <section class="aquarium-grid">
        <!-- Aquário de São Paulo -->
        <section class="aquarium-card">
