        </nav>
        <form class="search-box" action="/aquariosp" method="get">
        <input type="search" name="q" placeholder="Digite sua busca..." required>
        <button type="submit" class="buscartung">Buscar</button>
        </form>
        <nav>
            <ul>
                <li><a href="/login"><span class="logintexto">Fazer Login</span></a></li>
            </ul>
        </nav>
    </header>

    <!-- Navigation Back Button -->
    <nav class="nav-back"><a href="homedoispontozero">
        <button class="back-btn" aria-label="Voltar">
            <svg width="24" height="24" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2">
                <path d="M19 12H5"></path>
                <path d="M12 19l-7-7 7-7"></path>
            </svg>
        </button></a>
    </nav>

    <section id="menuToggle">
                  <input type="checkbox">
                  <span></span>
                  <span></span>
                  <span></span>
                          <ul id="menu">
