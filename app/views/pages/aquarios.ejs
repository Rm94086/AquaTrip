                        <span class="star filled">★</span>
                        <span class="star filled">★</span>
                    </section>
                </section>
                <p class="card-price">a partir de R$ 120 por pessoa</p>
            </section>
        </section>

        <!-- AquaRio -->
        <section class="aquarium-card">
